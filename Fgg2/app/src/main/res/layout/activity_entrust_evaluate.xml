                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_marginLeft="@dimen/hd_house_marginLeft"
                        android:gravity="center_vertical"
                        android:text="@string/ee_report"
                        android:textColor="@color/black"
                        android:textSize="@dimen/buy_numal_text_size"/>

                    <ImageView
                        android:layout_width="23dp"
                        android:layout_height="23dp"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/buy_margin"
                        android:src="@mipmap/ee_add_people"/>
                </LinearLayout>

                <View style="@style/rl_line"/>
            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/ee_report_relativeLayout2"
                android:layout_width="match_parent"
                android:layout_height="@dimen/buy_item_hight">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <TextView
                        android:visibility="gone"
                        android:id="@+id/ee_report_textview1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="@dimen/hd_house_marginLeft"
                        android:gravity="center_vertical"
                        android:textColor="@color/red"
                        android:text="@string/ee_report_textview1"
                        android:textSize="@dimen/buy_numal_text_size"/>

                    <TextView
                        android:id="@+id/ee_report_textview_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="@dimen/hd_house_marginLeft"
                        android:gravity="center_vertical"
                        android:textColor="@color/black"
                        android:textSize="@dimen/buy_numal_text_size"/>
                    <TextView
                        android:id="@+id/ee_report_textview_phone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="@dimen/hd_house_marginLeft"
                        android:gravity="center_vertical"
                        android:textColor="@color/black"
                        android:textSize="@dimen/buy_numal_text_size"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/buy_margin_small"
                    android:orientation="horizontal">


                    <ImageView
                        android:layout_width="15dp"
                        android:layout_height="20dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/buy_margin"
                        android:src="@mipmap/schedule_arrow"/>
                </LinearLayout>

                <View style="@style/rl_line"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/ee_information_relativeLayout"
                android:layout_width="match_parent"
                android:layout_height="@dimen/buy_item_hight">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="@dimen/buy_left_text_width"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_marginLeft="@dimen/hd_house_marginLeft"
                        android:gravity="center_vertical"
                        android:text="@string/ee_information"
                        android:textColor="@color/black"
                        android:textSize="@dimen/buy_numal_text_size"/>

                    <TextView
                        android:id="@+id/ee_information_value"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/hd_house_marginLeft"
                        android:layout_marginRight="@dimen/buy_item_hight"
                        android:gravity="center_vertical"
                        android:singleLine="true"
                        android:textColor="@color/black"
                        android:textSize="@dimen/buy_numal_text_size"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/buy_margin_small"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/ee_information_warning"
                        style="@style/Buy_Waning_Img"
                        android:src="@mipmap/buy_warning"/>

                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/buy_margin"

                        android:src="@mipmap/buy_dowm"/>
                </LinearLayout>


                <View style="@style/rl_line"/>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="100dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="@dimen/buy_left_text_width"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_marginLeft="@dimen/hd_house_marginLeft"
                        android:gravity="center_vertical"
                        android:text="@string/ee_mark"
                        android:textColor="@color/black"
                        android:textSize="@dimen/buy_numal_text_size"/>

                    <EditText
                        android:id="@+id/ee_mark_value"
                        style="@style/Buy_EditText"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/hd_house_marginLeft"
                        android:layout_marginRight="@dimen/buy_item_hight"
                        android:gravity="center_vertical"
                        android:textColor="@color/black"
                        android:singleLine="false"
                        android:hint="@string/ee_mark_value"
                        android:textSize="@dimen/buy_numal_text_size"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/margin_10"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/ee_mark_warning"
                        style="@style/Buy_Waning_Img"
                        android:src="@mipmap/buy_warning"/>

                </LinearLayout>

                <View style="@style/rl_line"/>
            </RelativeLayout>

            <CheckBox
                android:id="@+id/ee_checkBox"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="@dimen/margin_10"
                android:checked="true"
                android:text="@string/eb_checkBox2"
                android:textColor="@color/black"/>

            <Button
                android:id="@+id/ee_bt_enter"
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin_40"
                android:layout_margin="@dimen/margin_10"
                android:background="@drawable/button_selector_bule"
                android:text="@string/ee_bt_enter"
                android:textColor="@color/white"
                android:textSize="@dimen/text_size_16"
                />

        </LinearLayout>


    </ScrollView>


</LinearLayout>
